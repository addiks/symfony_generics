{
    "name": "addiks/symfony_generics",
    "license": "MIT",
    "type": "library",
    "description": "A collection of generic configurable PHP components",

    "autoload": {
        "psr-4": {
            "Addiks\\SymfonyGenerics\\Tests\\Unit\\": "tests/unit/",
            "Addiks\\SymfonyGenerics\\": "php/"
        }
    },

    "require": {
        "php": ">=7.1",
        "symfony/serializer": "^3.0 || ^4.0 || ^5.1",
        "symfony/http-foundation": "^v3.4 || ^v4.1 || ^5.1",
        "symfony/property-access": "^3.4.0|^4.1.0|^4.2.0 || ^5.1",
        "symfony/security-bundle": "^3.4.0|^4.1.0|^4.2.0 || ^5.1",
        "symfony/framework-bundle": "^3.4.0|^4.1.0|^4.2.0 || ^5.1",
        "doctrine/orm": "^2.5.0|^2.6.0|^2.7.0",
        "twig/twig": "^2.4.0|^2.5.0|^3.0.0",
        "webmozart/assert": "^1.3",
        "psr/container": "^1.0",
        "ytake/valueobjects": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "infection/infection": "^0.11.0",
        "vimeo/psalm": "^2.0",
        "phpstan/phpstan": "^0.11.7"
    }
}
