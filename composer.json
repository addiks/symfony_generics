        "webmozart/assert": "^1.3",
        "psr/container": "^1.0",
        "symfony/property-access": "^4.1",
        "doctrine/orm": "^2.5.0|^2.6.0",
        "twig/twig": "^2.5.0",
        "symfony/security": "^2.8.0|^3.4.0|^4.1.0|^4.2.0",
        "symfony/framework-bundle": "^2.8.0|^3.4.0|^4.1.0|^4.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "infection/infection": "^0.11.0",
        "vimeo/psalm": "^2.0"
    }
}
